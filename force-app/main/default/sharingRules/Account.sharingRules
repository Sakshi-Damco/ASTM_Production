        </accountSettings>
        <label>Sales ManagersA</label>
        <sharedTo>
            <role>SalesManagers</role>
        </sharedTo>
        <sharedFrom>
            <role>SalesManagers</role>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
